package classwork;
import java.util.*;

public class Question1 {

	public static void main(String[] args) {
		//1.FIND THE MINIMUM AND MAXIMUM ELEMENT IN AN ARRAY?
				int[] a={10,70,30,40,50};
				
				Arrays.sort(a);
				System.out.println("Minimum="+a[0]);
				System.out.println("maximum="+a[a.length-1]);
			}

}
